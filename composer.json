{
  "name": "tourze/smtp-mailer-bundle",
  "description": "SMTP Mailer Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "symfony/mailer": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/form": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "easycorp/easyadmin-bundle": "^4",
    "psr/log": "^3|^2|^1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\SMTPMailerBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\SMTPMailerBundle\\Tests\\": "tests"
    }
  }
}